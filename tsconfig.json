{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": false,
    "baseUrl": "src",
    "paths": {
      "assets/*": ["assets/*"],
      "helpers/*": ["helpers/*"],
      "components/*": ["components/*"],
      
    }
  },
  "include": ["src", "src/index.tsx"],
  "exclude": ["node_modules"]
}
